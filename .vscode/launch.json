        {
            "name": "moviecollectionapp",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
                "0.0.0.0:8000"
            ],
            "django": true,
            "console": "integratedTerminal",
            "cwd": "/home/target/Documents/MovieCollectionApp",
            "justMyCode": false,
            "env": {
                "DJANGO_SETTINGS_MODULE": "MovieCollectionApp.settings"
            },
            "python": "/home/target/.pyenv/versions/.venv-user/bin/python"
        },
        {
            "name": "asyncmoviecollectionapp",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "MovieCollectionApp.asgi:application",
                "--host",
                "0.0.0.0",
                "--port",
                "8000",
                "--reload"
            ],
            "django": true,
            "console": "integratedTerminal",
            "cwd": "/home/target/Documents/MovieCollectionApp",
            "env": {
                "DJANGO_SETTINGS_MODULE": "MovieCollectionApp.settings"
            },
            "python": "/home/target/.pyenv/versions/.venv-user/bin/python",
            "envFile": "/home/target/.pyenv/versions/3.11.2/envs/.venv-user",
            "justMyCode": false
        }
        
    ]
}